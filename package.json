{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 next build",
    "build:win": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "cross-env NODE_ENV=test mocha --recursive \"test/**/*.test.js\" --timeout 60000",
    "test:factory": "cross-env NODE_ENV=test mocha \"test/index.test.js\" --timeout 60000",
    "test:helper": "cross-env NODE_ENV=test mocha --recursive \"test/helper.test.js\" --timeout 60000",
    "test:walk": "cross-env NODE_ENV=test mocha --recursive \"test/walk-to-earn.test.js\" --timeout 60000",
    "test:walk:report": "cross-env NODE_ENV=test mocha \"test/wte-report.test.js\" --timeout 60000",
    "test:watch": "cross-env NODE_ENV=test mocha --recursive \"test/watch-to-earn.test.js\" --timeout 60000",
    "agenda": "node agenda-babel.js",
    "analyze": "ANALYZE=true next build",
    "service:giveaway-import": "cross-env DOTENV_PATH=.env.local NODE_ENV=development SERVICE_NAME=giveawaybrk node side-services/index.js",
    "version-major": "yarn version --major && git push origin main --tags",
    "version-minor": "yarn version --minor && git push origin main --tags",
    "version-patch": "yarn version --patch && git push origin main --tags",
    "rollback": "git checkout $npm_config_tag && git checkout -b rollback/$npm_config_tag && git push origin rollback/$npm_config_tag"
  },
  "dependencies": {
    "@babel/code-frame": "^7.22.5",
    "@babel/compat-data": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/generator": "^7.22.5",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@headlessui/tailwindcss": "^0.1.3",
    "eslint": "^8.43.0",
    "eslint-config-next": "^13.4.6",
    "eslint-config-prettier": "^8.8.0",
    "mocha": "^10.2.0",
    "mongo": "^0.1.0",
    "next": "latest",
    "phaser": "^3.80.1",
    "phaser-matter-collision-plugin": "^1.0.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.7",
    "reflect-metadata": "^0.1.13",
    "tailwind": "^4.0.0",
    "tsyringe": "^4.8.0",
    "tw-elements": "^1.0.0-beta2"
  },
  "devDependencies": {
    "@types/node": "20.3.1",
    "@types/react": "^18.2.13",
    "autoprefixer": "^10.4.14",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "postcss": "^8.4.24",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.0.7",
    "typescript": "5.1.3"
  }
}
